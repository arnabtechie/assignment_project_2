{
    "env": {
        "es2021": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "arrow-body-style": ["error", "as-needed"],
        "no-restricted-modules": "off",
        "consistent-return": "off",
        "import/extensions": "off",
        "no-return-await": "off",
        "func-names": "off",
        "no-param-reassign": "off",
        "prefer-destructuring": "off",
        "no-console": "off",
        "arrow-parens": ["error", "as-needed"]
    }
}
